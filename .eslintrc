{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "semi": [2, "never"],
        "no-debugger": 1,
        "no-console": 1,
        "react/prop-types": 1,
        "react/no-unused-prop-types": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "no-prototype-builtins": 0,
        "jsx-a11y/href-no-hash": 0,
        "no-underscore-dangle": 0,
        "jsx-a11y/anchor-has-content": 0
    },
    "globals": {
        "window": true,
        "document": true,
        "Headers": true,
        "FileReader": true
    }
}
